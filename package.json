{
  "name": "vue-number-input-spinner",
  "version": "2.2.0",
  "description": "A Vue number spinner component that only allows the user to type in numbers. Maintained fork of krystalcampioni/vue-number-input-spinner ",
  "author": "sergiopalacio <sergiopalaciobach@gmail.com>",
  "license": "MIT",
  "main": "dist/vue-number-input-spinner.min.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "serve": "webpack-dev-server --inline --hot --open --config build/webpack.dev.conf.js",
    "dev": "npm run development",
    "development": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=build/webpack.dev.conf.js",
    "build": "rimraf dist && webpack --progress --hide-modules --config build/webpack.prod.conf.js",
    "watch": "npm run development -- --watch",
    "lint": "eslint */*.vue */*.js --quiet"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krystalcampioni/vue-number-input-spinner"
  },
  "keywords": [
    "vue",
    "vuejs",
    "number",
    "numeric",
    "input",
    "spinner",
    "number input",
    "number spinner",
    "numeric input",
    "numeric spinner"
  ],
  "bugs": {
    "url": "https://github.com/sergiopalacio/vue-number-input-spinner/issues"
  },
  "homepage": "https://github.com/sergiopalacio/vue-number-input-spinner",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-plugin-html": "^2.0.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.7",
    "vue": "^2.6.11",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.6.3",
    "webpack": "^4.2.0",
    "webpack-dev-server": "^3.10.1",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "vue-highlightjs": "^1.3.3"
  }
}
